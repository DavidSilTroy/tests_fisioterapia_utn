<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tests_fisioterapia">

    <!--storage permission-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tests_fisioterapia">


        <activity android:name=".UI.activities.MainActivity"/>
        <activity android:name=".UI.activities.AddPatientActivity"/>
        <activity android:name=".UI.activities.EditPatientActivity"/>
        <activity android:name=".UI.activities.PatientInfoActivity"/>
        <activity android:name=".UI.activities.AddTestActivity"/>
        <activity android:name=".UI.activities.RegisterUserActivity"/>
        <activity android:name=".UI.activities.ForgotPasswordActivity"/>
        <activity android:name=".UI.activities.AddUserDataActivity"/>
        <activity android:name=".UI.activities.TestIntroActivity"/>
        <activity android:name=".UI.activities.TestResultActivity"/>
        <activity android:name=".UI.activities.PoweredByActivity"/>
        <activity android:name=".UI.activities.IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>